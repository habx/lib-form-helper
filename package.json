{
  "name": "@habx/lib-form-helper",
  "version": "6.6.0",
  "description": "Wrappers for react-final-form",
  "sideEffects": false,
  "main": "./dist/index.cjs.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/typings/index.d.ts",
  "scripts": {
    "clean": "rimraf ./dist",
    "build": "npm run clean && npm run build:esm && npm run build:cjs",
    "build:esm": "tsc",
    "build:cjs": "bili",
    "build:watch": "npm run clean && tsc --watch",
    "lint": "eslint \"src/**\"",
    "test": "node --icu-data-dir node_modules/full-icu node_modules/.bin/jest --config ./jest.config.js"
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/habx/lib-form-helper.git"
  },
  "author": "Flavien DELANGLE",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/habx/lib-form-helper/issues"
  },
  "homepage": "https://github.com/habx/lib-form-helper#readme",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix"
    ]
  },
  "peerDependencies": {
    "@habx/lib-client-intl": "^1.2.0",
    "@habx/ui-core": "^4.1.0",
    "final-form": "^4.18.7",
    "final-form-arrays": "^3.0.2",
    "lodash": "^4.17.15",
    "react": "^16.13.0",
    "react-final-form": "^6.3.5",
    "styled-components": "^5.2.0"
  },
  "dependencies": {
    "brace": "^0.11.1",
    "react-ace": "^9.1.4"
  },
  "devDependencies": {
    "@habx/eslint-config-client": "^5.3.1",
    "@habx/lib-client-intl": "^1.2.0",
    "@habx/ui-core": "^4.1.0",
    "@types/jest": "^26.0.7",
    "@types/lodash": "^4.14.120",
    "@types/react": "^16.8.17",
    "@types/styled-components": "^5.1.4",
    "bili": "^5.0.2",
    "final-form": "^4.18.7",
    "final-form-arrays": "^3.0.2",
    "full-icu": "^1.3.1",
    "husky": "^4.0.0",
    "jest": "^26.1.0",
    "lint-staged": "^10.0.0",
    "lodash": "^4.17.15",
    "react": "^16.13.0",
    "react-final-form": "^6.3.5",
    "rimraf": "^3.0.0",
    "rollup-plugin-typescript2": "^0.29.0",
    "ts-jest": "^26.1.3",
    "typescript": "^4.0.2"
  },
  "publishConfig": "https://npm.pkg.github.com/"
}
